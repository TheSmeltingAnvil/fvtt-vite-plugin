{
  "name": "@foundryvtt/vite-plugin",
  "version": "0.1.0",
  "description": "A Vite plugin for Foundry VTT",
  "license": "MIT",
  "keywords": [
    "foundryvtt",
    "foundryvtt-vite-plugin",
    "vite",
    "vite-plugin-fvtt",
    "vite-plugin-foundryvtt"
  ],
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "pnpm@10.6.0",
  "dependencies": {
    "fs-extra": "^11.3.2",
    "js-yaml": "^4.1.0",
    "minimatch": "^10.0.3",
    "mrmime": "^2.0.1",
    "picocolors": "^1.1.1",
    "vite": "^npm:rolldown-vite@7.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.8.1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-js": "github:eslint/js",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
